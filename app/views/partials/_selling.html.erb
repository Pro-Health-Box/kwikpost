<h1>Selling</h1>

<div id="masonry-container" class="transitions-enabled clearfix centered">
	<%= render @products %>
</div>

<script>
$(function(){

var $container = $('#masonry-container');

$container.imagesLoaded(function(){
  $container.masonry({
    itemSelector: '.box',
  });
});

});
</script>
